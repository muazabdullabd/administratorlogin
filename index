<script>
    function login() {
        const username = document.getElementById('username').value;
        const password = document.getElementById('password').value;

        if (username === 'muazabdulla.bd@gmail.com' && password === 'Muazabdulla3066') {
            // Show Menu, Home, and Hide Login Container
            document.getElementById('menu').style.display = 'block';
            document.getElementById('home').style.display = 'block'; // Show Home section
            document.getElementById('loginContainer').style.display = 'none';
            document.getElementById('dashboard').style.display = 'none'; // Hide Dashboard
        } else {
            alert('Invalid username or password');
        }
    }

    function showHome() {
        // Show Home and Hide Dashboard
        document.getElementById('home').style.display = 'block';
        document.getElementById('dashboard').style.display = 'none';
    }

    function showDashboard() {
        // Show Dashboard and Hide Home
        document.getElementById('dashboard').style.display = 'block';
        document.getElementById('home').style.display = 'none';
    }

    function logout() {
        // Hide Menu, Dashboard, and Home, Show Login Container
        document.getElementById('menu').style.display = 'none';
        document.getElementById('dashboard').style.display = 'none';
        document.getElementById('home').style.display = 'none';
        document.getElementById('loginContainer').style.display = 'flex';
    }
</script>

